[Unit]
Description=PHP Built-in Server on port 1453
After=network.target

[Service]
Type=simple
ExecStart=/usr/bin/php -d upload_max_filesize=500M -d post_max_size=500M -S localhost:1453 /home/topluyo_com/1453/router.php
Restart=always
User=root
WorkingDirectory=/home/topluyo_com/1453

[Install]
WantedBy=multi-user.target
